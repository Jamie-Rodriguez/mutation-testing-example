{
  "name": "mutation-testing-example",
  "version": "1.0.0",
  "description": "Demonstration of mutation testing and how it can be used to spot gaps in traditional test coverage",
  "keywords": [
    "mutation-testing",
    "testing"
  ],
  "license": "BSD-3-Clause",
  "author": "Jamie Rodriguez",
  "type": "module",
  "baseUrl": "./src",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint \"{src,tests}/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}\"",
    "lint:fix": "eslint \"{src,tests}/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}\" --fix",
    "test": "mocha --config .mocharc.json",
    "test:coverage": "odz run npm run test && odz report",
    "test:mutation": "stryker run"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@stryker-mutator/core": "9.5.1",
    "@stryker-mutator/mocha-runner": "9.5.1",
    "@stryker-mutator/typescript-checker": "9.5.1",
    "@types/chai": "5.2.3",
    "@types/mocha": "10.0.10",
    "chai": "6.2.2",
    "eslint": "10.0.2",
    "mocha": "11.7.5",
    "one-double-zero": "1.1.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  }
}
